
#include <stdio.h> 

int main() 
{ 
  char first_ch = 'f'; 
  char str[] = "freeCodeCamp"; 
  char user_ch = 'p'; 
  char user_str[40]= "This place is very picturesque"; 
  int found = 2015, curr = 2020; 
  float num = 19.99; 
  int numoc = 31; 
  int c = 28; 
  printf("\n");
  printf("%c\n", first_ch); 
  printf("%s\n", str); 
  printf("%c\n", user_ch); 
  printf("%s\n", user_str); 
  printf("%d\n", found); 
  printf("%i\n", curr); 
  printf("%f\n", num); 
  printf("%e\n", num); 
  printf("%o\n", numoc); 
  printf("%x\n", c); 
  return 0;
}
